import std {
    print,
}

export the99bottles = () {
    let i = 99;
    while (i > 0) {
        print(i);
        print(" bottles of beer on the wall\n");
        print(i);
        print(" bottles of beer\n");
        print("Take one down, pass it around");
        i = i - 1;
        print(i);
        print(" bottles of beer on the wall\n");
    }
};